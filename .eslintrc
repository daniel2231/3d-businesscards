{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 0,
        "import/prefer-default-export": 0,
        "import/extensions": 0,
        "no-restricted-globals": 0
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {}
}
